{
    "builds": [
      {
        "src": "api/index.php",
        "use": "@vercel/php"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      }
    ],
    "buildCommand": "apt-get update && apt-get install -y libssl1.0.0 libssl-dev && vercel build"
  }
  